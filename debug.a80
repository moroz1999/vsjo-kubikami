                        module	debug
print_hero_state
                            ld	a,(hero.hero_elevator)
                            cp	elevators.item_on_elevator
                            jr	z,print_elev

                            ld	a,(hero.hero_state)
                            cp	hero.state_void
                            jr	z,print_void

                            cp	hero.state_jump
                            jr	z,print_jump

                            cp	hero.state_swim
                            jr	z,print_swim

                            cp	hero.state_ground
                            jr	z,print_ground
                            ret

print_elev
                            ld	hl,text_elev
                            jr	print_state
print_jump
                            ld	hl,text_jump
                            jr	print_state
print_void
                            ld	hl,text_void
                            jr	print_state
print_ground
                            ld	hl,text_ground
                            jr	print_state
print_swim
                            ld	hl,text_swim
                            jr	print_state
print_state
                            ld	b,25
                            ld	c,23
                            call	print.print_string
                            ret
text_void
                            db	"net   ",255
text_jump
                            db	"pryvok",255
text_swim
                            db	"woda  ",255
text_ground
                            db	"zemlq ",255
text_elev
                            db	"lift  ",255
                        endmodule